{
  "name": "shinezone-generator",
  "version": "1.0.0",
  "description": "react16+react-router4+mobx+ssr",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "start": "better-npm-run start",
    "start:dist": "better-npm-run start:dist",
    "start:dist:server": "better-npm-run start:dist:server",
    "build:dev:deploy": "better-npm-run build:dev:deploy",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "deploy:prod:server": "better-npm-run deploy:prod:server",
    "server": "better-npm-run server",
    "publish:setup": "./node_modules/.bin/pm2 deploy ecosystem.config.js development setup",
    "publish": "./node_modules/.bin/pm2 deploy ecosystem.config.js development",
    "test": "cross-env NODE_ENV=development karma start build/karma.conf.js",
    "test:watch": "npm run test --watch",
    "lint:fix": "npm run lint -- --fix",
    "lint": "./node_modules/.bin/tslint -c tslint.json -p tsconfig.json --fix --format stylish"
  },
  "betterScripts": {
    "start": {
      "command": "babel-node build/client/index.js",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "start:dist": {
      "command": "babel-node build/client/index.js",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start:dist:server": {
      "command": "ts-node build/server/index.js",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*",
        "RENDER_TYPE": "server"
      }
    },
    "server": {
      "command": "npm run deploy:prod:server && babel-node build/server/index.js",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*",
        "RENDER_TYPE": "server"
      }
    },
    "compile": {
      "command": "babel-node build/compile.js",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod:server": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*",
        "RENDER_TYPE": "server"
      }
    }
  },
  "author": "shienzone",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.0.39",
    "@types/webpack": "^3.0.14",
    "@types/webpack-env": "^1.13.2",
    "awesome-typescript-loader": "^3.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "compression": "^1.7.1",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "fork-ts-checker-notifier-webpack-plugin": "^0.2.0",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "fs-extra": "^4.0.2",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "jasmine": "^2.8.0",
    "koa": "^2.3.0",
    "koa-compress": "^2.0.0",
    "koa-connect-history-api-fallback": "^0.3.1",
    "koa-convert": "^1.2.0",
    "koa-static": "^4.0.1",
    "loader-runner": "^2.3.0",
    "lodash": "^4.17.4",
    "mobx": "^3.3.1",
    "mobx-react": "^4.3.4",
    "mobx-react-devtools": "^4.2.15",
    "mobx-react-router": "^4.0.1",
    "morgan": "^1.9.0",
    "object-assign": "^4.1.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.1",
    "react-immutable-render-mixin": "^0.9.7",
    "react-router-dom": "^4.2.2",
    "rimraf": "^2.6.2",
    "ts-loader": "^3.1.1",
    "typescript": "^2.6.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "better-npm-run": "^0.1.0",
    "webpack": "^3.8.1"
  }
}
