{
  "presets": [
    "env",
    "react"
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "helpers": true,
        "polyfill": true,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ],
    [
      "antd",
      {
        "style": true
      }
    ],
    "transform-decorators-legacy",
    "babel-plugin-transform-object-assign"
  ]
}
